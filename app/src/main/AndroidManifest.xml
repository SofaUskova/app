<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.uiClasses.GenerateList"
            android:label="" />
        <activity
            android:name=".ui.uiClasses.FilterActivity"
            android:label="Фильтр" />
        <activity
            android:name=".ui.uiClasses.DetailInformationActivity"
            android:label="" />
        <activity
            android:name=".ui.uiClasses.ViewingImagesActivity"
            android:label=""
            android:theme="@style/AppThemeDark" />
        <activity android:name=".ui.MainActivity"
            android:label="" />
        <activity android:name=".ui.login.RegistrationActivity"
            android:label="" />
        <activity
            android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>